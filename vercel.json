{
  "buildCommand": "prisma generate && next build",
  "installCommand": "npm install",
  "env": {
    "PRISMA_GENERATE_SKIP_AUTOINSTALL": "true"
  },
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "regions": ["bom1", "sin1"],
  "framework": "nextjs",
  "outputDirectory": ".next"
}
